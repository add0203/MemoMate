<div class="container-fluid pd-5 mb-5">
  <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if(searchResults != ''){%> <% searchResults.forEach((note, index)=>{%>
    <a href="/dashboard/item/<%= note._id %>">
      <h4><%= note.title %></h4>
    </a>
    <% })%> <%} else {%>
    <p>No Item Found.</p>
    <%}%>
  </div>
</div>
